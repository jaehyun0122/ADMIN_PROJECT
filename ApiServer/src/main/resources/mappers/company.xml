<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.apiserver.dao.CompanyDao">
    <select id="getCompanyCd" resultType="boolean">
        SELECT EXISTS(SELECT 1 FROM company WHERE companyCd = #{companyCd});
    </select>
</mapper>